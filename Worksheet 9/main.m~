 function a = linterp_poly(X, Y)
  a = zeros(1,numel(X));
  for i = 1:numel(X)
      aa = poly(X([1:i-1 i+1:end]));
      a = a + Y(i) * aa / polyval(aa, X(i));
  end
end


function y = num_diff(X,Y,x,d)
    lagrange_coeff = linterp_poly[X,Y];
    derive_coefficients = lagrange_coeff;
    for i = 1:d
        derive_coefficients = polyd
    end
end